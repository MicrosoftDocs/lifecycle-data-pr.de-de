{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/lifecycle/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {
      "author": {
        "**/*.yml": "github-name",
        "**/*.md": "github-name"
      },
      "ms.author": {
        "**/*.yml": "mslpol",
        "**/*.md": "mslpol"
      },
      "ms.prod": {
        "**/*.yml": "lifecycle",
        "**/*.md": "lifecycle"
      },
      "ms.topic": {
        "**/*.yml": "lifecycle",
        "**/*.md": "lifecycle"
      },
      "titleSuffix": {
        "**/*.yml": "Microsoft Lifecycle",
        "**/*.md": "Microsoft Lifecycle"
      }
    },
    "template": [],
    "dest": "lifecycle-data",
    "markdownEngineName": "markdig"
  }
}